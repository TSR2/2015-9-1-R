i=i+1
scount=0
}
}
persent=sum(list=="F")/n
list(list,persent,mtotal)
}
###########################################
ifail=function(a=1,b=1,n=100,alpha=0.1,t=0){
if (t==1){rb=createbeta(n,alpha)}
else {rb=rbeta(n = n,shape1 = a,shape2 = b)}
list=character(n);p=0
i=1;ru=runif(n);Fcount=0
ru=runif(n)
for (j in 1:n){
taget=rb[i]
if (taget>=ru[j]) {p="S"
}else {p="F"
Fcount=Fcount+1}
list[j]=p
if (Fcount==i) {
i=i+1
Fcount=0
}
}
persent=sum(list=="F")/n
list(list,persent)
}
